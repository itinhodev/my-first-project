Algoritmo "SomaDosValores"

Var
   cont, n, s: Inteiro

Inicio
      cont <- 1
      s <- 0
      Enquanto (cont <= 5) faca
      Escreva ("Digite o", cont, "º número: ")
      Leia (n)
      s <- s + n
      cont <- cont + 1
      FimEnquanto
      Escreval ("A soma de todos os números foi:", s)

Fimalgoritmo